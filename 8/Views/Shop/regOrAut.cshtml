
@model WebApplication8.Models.Users
<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
</head>

<h2>Введите свои данные</h2>

<body class="text-center" cz-shortcut-listen="true">

    <main class="form-signin w-100 m-auto">
        @using (Html.BeginForm())
        {

            @Html.LabelFor(model => model.name, "Имя")

            <br />
            @Html.EditorFor(model => model.name)
            <br />
            <br />
            @Html.LabelFor(model => model.email, "Почта")
            <br />
            @Html.EditorFor(model => model.email)
            <br />
            <br />
            @Html.LabelFor(model => model.phone, "Номер телефона")
            <br />
            @Html.EditorFor(model => model.phone)
            <br />
            <br />
            <input class="w-100 btn btn-lg btn-primary" type="submit" value="Войти" />
        }
    </main>





</body>
